{
  "name": "investments-price-alert-root-backend",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/investments-price-alert/backend/root-backend/src",
  "projectType": "application",
  "targets": {
    "migration-create": {
      "command": "npx typeorm migration:create apps/investments-price-alert/backend/root-backend/src/migrations/migration-name-here"
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack-cli build",
        "args": ["node-env=production"]
      },
      "configurations": {
        "development": {
          "args": ["node-env=development"]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "investments-price-alert-root-backend:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "investments-price-alert-root-backend:build:development"
        },
        "production": {
          "buildTarget": "investments-price-alert-root-backend:build:production"
        }
      }
    }
  },
  "tags": []
}
